<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">
    <!--
        Added the constraints for entity ProcedureHistory.
    -->
    <changeSet id="created_entity_constraints_ProcedureHistory_v1.0" author="m.kerabandi">

        <addForeignKeyConstraint baseColumnNames="id_proc"
                                 baseTableName="tb_ce_proc_storico"
                                 constraintName="fk_proc_storico_proc"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_ce_procedura"/>

        <addForeignKeyConstraint baseColumnNames="id_stato_procedura"
                                 baseTableName="tb_ce_proc_storico"
                                 constraintName="fk_stato_storico_proc"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_ce_stato_procedura"/>
    </changeSet>
</databaseChangeLog>
