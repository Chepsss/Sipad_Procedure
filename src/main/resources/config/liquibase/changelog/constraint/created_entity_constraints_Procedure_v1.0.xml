<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">
    <!--
        Added the constraints for entity Procedure.
    -->
    <changeSet id="created_entity_constraints_Procedure_v1.0" author="m.kerabandi">

        <addForeignKeyConstraint baseColumnNames="id_stato_procedura"
                                 baseTableName="tb_ce_procedura"
                                 constraintName="fk_procedure__state_procedure_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_ce_stato_procedura"/>

        <addForeignKeyConstraint baseColumnNames="id_tipo_cessazione"
                                 baseTableName="tb_ce_procedura"
                                 constraintName="fk_procedure__tipo_cess_id"
                                 referencedColumnNames="cetip_id_pk"
                                 referencedTableName="tp_cetip_cessazione"/>

        <addForeignKeyConstraint baseColumnNames="id_tipo_gml_1"
                                 baseTableName="tb_ce_procedura"
                                 constraintName="fk_procedure__giumlegale1_id"
                                 referencedColumnNames="cegml_cod_pk"
                                 referencedTableName="tp_cegml_giud_med_legale"/>

        <addForeignKeyConstraint baseColumnNames="id_tipo_gml_2"
                                 baseTableName="tb_ce_procedura"
                                 constraintName="fk_procedure__giumlegale2_id"
                                 referencedColumnNames="cegml_cod_pk"
                                 referencedTableName="tp_cegml_giud_med_legale"/>

    </changeSet>
</databaseChangeLog>
